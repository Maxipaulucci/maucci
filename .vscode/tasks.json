{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "maven-package",
            "type": "shell",
            "command": "mvn",
            "args": [
                "clean",
                "package",
                "-DskipTests"
            ],
            "options": {
                "cwd": "${workspaceFolder}/backend"
            },
            "problemMatcher": []
        },
        {
            "label": "maven-spring-boot-run",
            "type": "shell",
            "command": "mvn",
            "args": [
                "spring-boot:run"
            ],
            "options": {
                "cwd": "${workspaceFolder}/backend"
            },
            "problemMatcher": []
        }
    ]
}











